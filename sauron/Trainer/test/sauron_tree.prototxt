  bottom: "fc0-3"
  top: "fc0-3"
}
layer {
  name: "drop0-3"
  type: "Dropout"
  bottom: "fc0-3"
  top: "fc0-3"
  dropout_param {
    dropout_ratio: 0.2
  }
}
