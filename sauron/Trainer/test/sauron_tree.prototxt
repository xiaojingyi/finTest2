  name: "drop-final-prev"
  type: "Dropout"
  bottom: "final-prev"
  top: "final-prev"
  dropout_param {
    dropout_ratio: 0.5
  }
}
layer {
  name: "concat"
  type: "Concat"
  bottom: "confc0-3"
  bottom: "confc1-3"
  top: "concat"
}
