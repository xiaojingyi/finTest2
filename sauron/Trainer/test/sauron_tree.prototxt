name: "LogisticRegressionNet"
layer {
  name: "data"
  type: "HDF5Data"
  top: "data"
  top: "label"
  include {
    phase: TRAIN
  }
  hdf5_data_param {
    source: "test/data/train.txt"
    batch_size: 256
  }
}
layer {
  name: "data"
  type: "HDF5Data"
  top: "data"
  top: "label"
  include {
    phase: TEST
  }
  hdf5_data_param {
    source: "test/data/test.txt"
    batch_size: 10
  }
}

##################### spliter
layer {
  name: "slice_pair"
  type: "Slice"
  bottom: "data"
  top: "data0"
  top: "data1"
  top: "data2"
  top: "data3"
  top: "data4"
  top: "data5"
  slice_param {
    slice_dim: 1
    slice_point: 600
    slice_point: 1200
    slice_point: 1800
    slice_point: 2400
    slice_point: 3000
  }
}

################## fc0
layer {
  name: "fc0-1"
  type: "InnerProduct"
  bottom: "data0"
  top: "fc0-1"
  param {
    lr_mult: 1
    decay_mult: 1
  }
  param {
    lr_mult: 2
    decay_mult: 0
  }
  inner_product_param {
    num_output: 512
    weight_filler {
      type: "xavier"
      std: 0.01
    }
    bias_filler {
      type: "constant"
      value: 0
    }
  }
}
layer {
  name: "relu0-1"
  type: "ReLU"
  bottom: "fc0-1"
  top: "fc0-1"
}
layer {
  name: "drop0-1"
  type: "Dropout"
  bottom: "fc0-1"
  top: "fc0-1"
  dropout_param {
    dropout_ratio: 0.5
  }
}
layer {
  name: "fc0-2"
  type: "InnerProduct"
  bottom: "fc0-1"
  top: "fc0-2"
  param {
    lr_mult: 1
    decay_mult: 1
  }
  param {
    lr_mult: 2
    decay_mult: 0
  }
  inner_product_param {
    num_output: 256
    weight_filler {
      type: "xavier"
      std: 0.01
    }
    bias_filler {
      type: "constant"
      value: 0
    }
  }
}
layer {
  name: "relu0-2"
  type: "ReLU"
  bottom: "fc0-2"
  top: "fc0-2"
}
layer {
  name: "drop0-2"
  type: "Dropout"
  bottom: "fc0-2"
  top: "fc0-2"
  dropout_param {
    dropout_ratio: 0.5
  }
}
layer {
  name: "fc0-3"
  type: "InnerProduct"
  bottom: "fc0-2"
  top: "fc0-3"
  param {
    lr_mult: 1
    decay_mult: 1
  }
  param {
    lr_mult: 2
    decay_mult: 0
  }
  inner_product_param {
    num_output: 128
    weight_filler {
      type: "xavier"
      std: 0.01
    }
    bias_filler {
      type: "constant"
      value: 0
    }
  }
}
layer {
  name: "relu0-3"
