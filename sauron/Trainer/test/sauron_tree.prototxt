# --------------------------------------- concat split ---------------------------------------
layer {
  name: "concat0"
  type: "Concat"
  bottom: "fc0-3"
  bottom: "fc1-3"
  bottom: "fc2-3"
  top: "concat0"
}
layer {
  name: "concat1"
  type: "Concat"
  bottom: "fc3-3"
  bottom: "fc4-3"
  bottom: "fc5-3"
  top: "concat1"
}
################### confc0
layer {
  name: "confc0-1"
  type: "InnerProduct"
  bottom: "concat0"
  top: "confc0-1"
  param {
    lr_mult: 1
    decay_mult: 1
  }
  param {
    lr_mult: 2
    decay_mult: 0
  }
  inner_product_param {
    num_output: 256
    weight_filler {
      type: "xavier"
      std: 0.01
    }
    bias_filler {
      type: "constant"
      value: 0
    }
  }
}
layer {
  name: "confc0-relu1"
  type: "ReLU"
  bottom: "confc0-1"
  top: "confc0-1"
}
layer {
  name: "confc0-drop1"
  type: "Dropout"
  bottom: "confc0-1"
  top: "confc0-1"
  dropout_param {
    dropout_ratio: 0.3
  }
}
layer {
  name: "confc0-2"
  type: "InnerProduct"
  bottom: "confc0-1"
  top: "confc0-2"
  param {
    lr_mult: 1
    decay_mult: 1
  }
  param {
    lr_mult: 2
    decay_mult: 0
  }
  inner_product_param {
    num_output: 128
    weight_filler {
      type: "xavier"
      std: 0.01
    }
    bias_filler {
      type: "constant"
      value: 0
    }
  }
}
layer {
  name: "confc0-relu2"
  type: "ReLU"
  bottom: "confc0-2"
  top: "confc0-2"
}
layer {
  name: "confc0-drop2"
  type: "Dropout"
  bottom: "confc0-2"
