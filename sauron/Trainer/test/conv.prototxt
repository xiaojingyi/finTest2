name: "LogisticRegressionNet"
layer {
  name: "data"
  type: "HDF5Data"
  top: "data"
  top: "label"
  include {
    phase: TRAIN
  }
  hdf5_data_param {
    source: "test/data/train.txt"
    batch_size: 256
  }
}
layer {
  name: "data"
  type: "HDF5Data"
  top: "data"
  top: "label"
  include {
    phase: TEST
  }
  hdf5_data_param {
    source: "test/data/test.txt"
    batch_size: 10
  }
}
layer {
  name: "reshape"
  type: "Reshape"
  bottom: "data"
  top: "reshape"
  reshape_param {
    shape {
      dim: 0  # copy the dimension from below
      dim: 1
      dim: 1
      dim: -1 # infer it from the other dimensions
    }
  }
}
layer {
  name: "conv1"
  type: "Convolution"
  bottom: "reshape"
  top: "conv1"
  param {
    lr_mult: 1 # first
    decay_mult: 1
  }
  param {
    lr_mult: 2 # second
    decay_mult: 0
  }
  convolution_param {
    num_output: 128
    pad: 0
    kernel_w: 7
    kernel_h: 1
    stride_w: 2
    stride_h: 1
    weight_filler {
      type: "xavier"
    }
    bias_filler {
      type: "constant"
      value: 0
    }
  }
}
layer {
  name: "relu1"
  type: "ReLU"
  bottom: "conv1"
  top: "conv1"
}
layer {
  name: "conv2"
  type: "Convolution"
  bottom: "conv1"
  top: "conv2"
  param {
    lr_mult: 1 # first
    decay_mult: 1
  }
  param {
    lr_mult: 2 # second
    decay_mult: 0
  }
  convolution_param {
    num_output: 128
    pad: 0
    kernel_w: 10
    kernel_h: 1
    stride_w: 1
    stride_h: 1
    weight_filler {
      type: "xavier"
    }
    bias_filler {
      type: "constant"
      value: 0
    }
  }
}
layer {
  name: "relu2"
  type: "ReLU"
  bottom: "conv2"
  top: "conv2"
}
layer {
  name: "conv3"
  type: "Convolution"
  bottom: "conv2"
  top: "conv3"
  param {
    lr_mult: 1 # first
    decay_mult: 1
  }
  param {
    lr_mult: 2 # second
    decay_mult: 0
  }
  convolution_param {
    num_output: 128
    pad: 0
    kernel_w: 10
    kernel_h: 1
    stride_w: 1
    stride_h: 1
    weight_filler {
      type: "xavier"
    }
    bias_filler {
      type: "constant"
      value: 0
    }
  }
}
layer {
  name: "relu3"
  type: "ReLU"
  bottom: "conv3"
  top: "conv3"
}
layer {
  name: "conv4"
  type: "Convolution"
  bottom: "conv3"
  top: "conv4"
  param {
    lr_mult: 1 # first
    decay_mult: 1
  }
  param {
    lr_mult: 2 # second
    decay_mult: 0
  }
  convolution_param {
    num_output: 128
    pad: 0
    kernel_w: 5
    kernel_h: 1
    stride_w: 1
    stride_h: 1
    weight_filler {
      type: "xavier"
    }
    bias_filler {
      type: "constant"
      value: 0
    }
  }
}
layer {
  name: "relu4"
  type: "ReLU"
  bottom: "conv4"
  top: "conv4"
}

######################################## helper
layer {
  name: "helper_prev"
  type: "InnerProduct"
  bottom: "conv2"
  top: "helper_prev"
  param {
    lr_mult: 1
    decay_mult: 1
  }
  param {
    lr_mult: 2
    decay_mult: 0
  }
  inner_product_param {
    num_output: 128
    weight_filler {
      type: "xavier"
    }
    bias_filler {
      type: "constant"
      value: 0
    }
  }
}
layer {
  name: "helper"
  type: "InnerProduct"
  bottom: "helper_prev"
  top: "helper"
  param {
    lr_mult: 1
    decay_mult: 1
  }
  param {
    lr_mult: 2
    decay_mult: 0
  }
  inner_product_param {
    num_output: 3600
    weight_filler {
      type: "gaussian"
      std: 1
      sparse: 15
    }
    bias_filler {
      type: "constant"
      value: 0
    }
  }
}
layer {
  name: "loss_helper"
  type: "EuclideanLoss"
  bottom: "helper"
  bottom: "data"
  top: "loss_helper"
  loss_weight: 0.001
}

################################################## outer
layer {
  name: "out"
  type: "InnerProduct"
  bottom: "conv4"
  top: "out"
  param {
    lr_mult: 1
    decay_mult: 1
  }
  param {
    lr_mult: 2
    decay_mult: 0
  }
  inner_product_param {
    num_output: 10
    weight_filler {
      type: "gaussian"
      std: 0.003
    }
    bias_filler {
      type: "constant"
      value: 0
    }
  }
}
layer {
  name: "loss"
  type: "SoftmaxWithLoss"
  bottom: "out"
  bottom: "label"
  top: "loss"
  loss_weight: 1
}
layer {
  name: "accuracy-train"
  type: "Accuracy"
  bottom: "out"
  bottom: "label"
  top: "accuracy-train"
  include {
    phase: TRAIN
  }
}
layer {
  name: "accuracy"
  type: "Accuracy"
  bottom: "out"
  bottom: "label"
  top: "accuracy"
  include {
    phase: TEST
  }
}

